{
  "name": "MSGV-suffle",
  "version": "1.0.0",
  "description": "'shuffles' your MGSV helicopter music",
  "private": true,
  "main": "MGSV-shuffle.js",
  "bin": {
    "MGSV-shuffle": "./bin/MGSV-shuffle"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "nexe"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tylergoodman/MGSV-shuffle.git"
  },
  "bugs": {
    "url": "https://github.com/tylergoodman/MGSV-shuffle/issues"
  },
  "homepage": "https://github.com/tylergoodman/MGSV-shuffle",
  "devDependencies": {
    "nexe": "^0.4.1"
  },
  "dependencies": {},
  "nexe": {
    "input": "./MGSV-shuffle.js",
    "output": "./bin/MGSV-shuffle^$",
    "runtime": {
      "framework": "nodejs",
      "version": "4.0.0",
      "ignoreFlags": true
    }
  }
}
