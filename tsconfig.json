{
    "version": "1.5.0-beta",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": false,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*",
        "!./bin/**/*"
    ],
    "files": [
        "./src/index.ts",
        "./typings/github-electron/github-electron.d.ts",
        "./typings/node/node.d.ts",
        "./typings/react/react.d.ts",
        "./typings/tsd.d.ts",
        "./src/app/FSFilesList.tsx",
        "./src/app/FSInput.tsx",
        "./src/app/index.tsx",
        "./src/main.tsx"
    ]
}
